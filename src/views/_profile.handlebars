<div>
    <h1>Profile</h1>
    <h2>Bienvenido a tu perfil {{userData.user}}.</h2><br>
    <p>Tu tipo de usuario es: {{userData.rol}} </p>
    <p>{{userData.user.cart.object._id}}</p>

    <h1>Vista del carrito</h1>
{{#if _id}}
    {{#if products}}
        <h2>Listado de productos agregados</h2>
        {{#each products}}
            <div>
                <p>{{this.id.title}}</p>
                <p>Cantidad: {{this.quantity}}</p>
            </div>
        {{/each}}
    {{else}}
        No hay productos en el carrito
    {{/if}}
{{else}}
    <p>No hay detalles</p>
{{/if}}
</div>

